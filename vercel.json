{
    "installCommand" : "npm install --legacy-peer-deeps",
    "version" : 2,
    "builds" : [
        {
            "src": "server.js",
            "use" : "@vercel/node"
        },
        {
            "src" : "src/**/*",
            "use" : "@vercel/static"
        }
    ],
    "routes": [
    { "src": "/api/(.*)", "dest": "/backend/server.js" },
    { "src": "/static/(.*)", "dest": "/src/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
]
}